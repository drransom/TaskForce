<% var submitButton = submitButton || false %>
  <div class="col-md-4 mini-profile">
    <div class="thumbnail">
      <img src="<%= model.get('profile_url')%>"  class="profile-img">
    </div>
    <ul class="list-group">
      <li class="list-group-item"><%= model.get('name') %></li>
      <li class="list-group-item"><%= TaskForce.Helpers.taskerRating(model) %>
      &nbsp;<%= model.get('num_completed') %> completed</li>
    </ul>
    <% if (submitButton) { %>
      <%= TaskForce.Helpers.hireButton(model) %>
      <a href="javascript:void(0)" data-id="<%= model.get('id') %>" class="tasker-profile">See reviews & profile</a>
    <% } %>
  </div>
</div>
